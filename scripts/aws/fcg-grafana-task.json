{
  "family": "fcg-grafana-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "LabRole",
  "containerDefinitions": [
    {
      "name": "fcg-grafana",
      "image": "",
      "portMappings":[{"containerPort":3000,"protocol":"tcp"}],
      "environment":[
        {"name":"GF_SECURITY_ADMIN_PASSWORD","value":""},
        {"name":"GF_USERS_ALLOW_SIGN_UP","value":"false"}
      ],
      "essential":true,
      "logConfiguration":{"logDriver":"awslogs","options":{"awslogs-group":"/ecs/fcg-grafana","awslogs-region":"us-east-1","awslogs-stream-prefix":"ecs"}}
    }
  ]
}
