{
  "family": "fcg-prometheus-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "LabRole",
  "containerDefinitions": [
    {
      "name": "fcg-prometheus",
      "image": "",
      "portMappings":[{"containerPort":9090,"protocol":"tcp"}],
      "essential":true,
      "logConfiguration":{"logDriver":"awslogs","options":{"awslogs-group":"/ecs/fcg-prometheus","awslogs-region":"us-east-1","awslogs-stream-prefix":"ecs"}}
    }
  ]
}
